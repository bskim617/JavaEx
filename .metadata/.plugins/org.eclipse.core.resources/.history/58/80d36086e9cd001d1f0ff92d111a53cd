package ch03;

public class OperationEx5 {
	//비트 이동 연산자를 사용하여 연산하기
	public static void main(String[] args) {
		int num = 0B00000101; //5를 8비트 2진수로 나타냄
		
		System.out.println(num << 2);
		System.out.println(num >> 2);
		System.out.println(num >>> 2);
		
		System.out.println(num);
		
		num = num << 2;
		System.out.println(num);
		
	}
}
